{
    "name": "shomisha/stubless",
    "description": "A package for generating PHP files without stubs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miša Ković",
            "email": "misa95kovic@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "nikic/php-parser": "^4.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Shomisha\\Stubless\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shomisha\\Stubless\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit --bootstrap vendor/autoload.php --config phpunit.xml"
    }
}
